<?xml version="1.0" encoding="UTF-8"?>
<root>
	<element name="Window">
		<font></font>
		<attributes>
			<property name="resizeBorders" type="Vector4f">
				<x value="14" />
				<y value="14" />
				<z value="14" />
				<w value="14" />
			</property>
			<property name="minSize" type="Vector2f">
				<x value="150" />
				<y value="50" />
			</property>
			<property name="defaultSize" type="Vector2f">
				<x value="350" />
				<y value="200" />
			</property>
		</attributes>
		<images>
			<property name="defaultImg" type="String" value="tonegod/gui/style/def/Window/panel_x.png" />
		</images>
		<effects>
			<property name="event0" type="Effect">
				<event value="Show" />
				<effect value="FadeIn" />
				<speed value="1.5" />
			</property>
		</effects>
	</element>

	<element name="Window#Dragbar">
		<font>
			<property name="fontSize" type="float" value="18" />
			<property name="fontColor" type="ColorRGBA">
				<r value="1.0" />
				<g value="1.0" />
				<b value="1.0" />
				<a value="1.0" />
			</property>
			<property name="textAlign" type="String" value="Left" />
			<property name="textWrap" type="String" value="Clip" />
			<property name="textPadding" type="float" value="3" />
		</font>
		<attributes>
			<property name="resizeBorders" type="Vector4f">
				<x value="6" />
				<y value="6" />
				<z value="6" />
				<w value="6" />
			</property>
		</attributes>
		<images>
			<property name="defaultImg" type="String" value="tonegod/gui/style/def/Window/dragbar_x.png" />
		</images>
		<effects></effects>
	</element>
</root>
